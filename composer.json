{
  "name": "dcsg/php-immutable-collections",
  "type": "library",
  "description": "Minimalist and Typed Immutable Collections",
  "keywords": [
    "collection",
    "immutable",
    "immutable-collection",
    "pipe"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Gomes",
      "email": "danielcesargomes@gmail.com",
      "homepage": "https://dcsg.me"
    }
  ],
  "require": {
    "php": "^7.2|^7.4|^8.0|^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "DCSG\\ImmutableCollections\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DCSG\\ImmutableCollections\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "check-style": "./vendor/bin/phpcs src tests",
    "fix-style": "./vendor/bin/phpcbf src tests"
  }
}
